<?xml version="1.0" encoding="utf-8"?>
<MessageDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Services>
  	<Service>
  	  <Name>skywave15</Name>
  	  <SIN>15</SIN>
  	  <ReturnMessages>
  	  	<Message>
  	  	  <Name>modemLocked</Name>
  	  	  <Description>Scrambled message</Description>
  	  	  <MIN>255</MIN>
  	  	</Message>
  	  </ReturnMessages>
  	</Service>
    <Service>
      <Name>system</Name>
      <SIN>16</SIN>
      <ForwardMessages>
        <Message>
          <Name>getTerminalInfo</Name>
          <Description>Request terminal information. A terminalInfo message is generated in response.</Description>
          <MIN>1</MIN>
        </Message>
        <Message>
          <Name>getTerminalStatus</Name>
          <Description>Request terminal status. A terminalStatus message is generated in response.</Description>
          <MIN>2</MIN>
        </Message>
        <Message>
          <Name>getServiceList</Name>
          <Description>Request a list of all services. A serviceList message is generated in response.</Description>
          <MIN>3</MIN>
        </Message>
        <Message>
          <Name>getServiceInfo</Name>
          <Description>Request information for one or more services. A serviceInfo message is generated in response.</Description>
          <MIN>4</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>sin</Name>
              <Size>8</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>restartService</Name>
          <Description>Request to restart a service.</Description>
          <MIN>5</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>sin</Name>
              <Size>8</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>resetTerminal</Name>
          <Description>Request to restart the terminal. If the resetAll field is True, also resets all hardware.</Description>
          <MIN>6</MIN>
          <Fields>
            <Field xsi:type="BooleanField">
              <Name>resetAll</Name>
              <Description>If true, all hardware is reset in addition to the terminal.</Description>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>getTerminalMetrics</Name>
          <Description>Request current terminal metrics. Currently, no message is generated in response, since no metrics have been defined.</Description>
          <MIN>7</MIN>
        </Message>
        <Message>
          <Name>getProperties</Name>
          <Description>Request the value of one or more properties. A propertyValues message is generated in response.</Description>
          <MIN>8</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Description>If not provided, or size is 0, get properties from all services.</Description>
              <Optional>true</Optional>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="DataField">
                  <Name>pinList</Name>
                  <Description>If size is 0, get all properties from that service.</Description>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setProperties</Name>
          <Description>Sets the value of one or more properties.</Description>
          <MIN>9</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="ArrayField">
                  <Name>propList</Name>
                  <Fields>
                    <Field xsi:type="UnsignedIntField">
                      <Name>pin</Name>
                      <Size>8</Size>
                    </Field>
                    <Field xsi:type="DynamicField">
                      <Name>value</Name>
                    </Field>
                  </Fields>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>save</Name>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>resetProperties</Name>
          <Description>Resets one or more properties to their default values.</Description>
          <MIN>10</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Description>If not provided, or size is 0, reset properties for all services.</Description>
              <Optional>true</Optional>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="DataField">
                  <Name>pinList</Name>
                  <Description>If size is 0, reset all properties for that service.</Description>
                  <Optional>true</Optional>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>saveProperties</Name>
          <Description>Saves one or more property values to non-volatile storage.</Description>
          <MIN>11</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Description>If not provided, or size is 0, save properties for all services.</Description>
              <Optional>true</Optional>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="DataField">
                  <Name>pinList</Name>
                  <Description>If size is 0, save all properties for that service.</Description>
                  <Optional>true</Optional>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>revertProperties</Name>
          <Description>Reverts one or more property values from non-volatile storage.</Description>
          <MIN>12</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Description>If not provided, or size is 0, revert properties for all services.</Description>
              <Optional>true</Optional>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="DataField">
                  <Name>pinList</Name>
                  <Description>If size is 0, revert all properties for that service.</Description>
                  <Optional>true</Optional>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>restartFramework</Name>
          <Description>Restarts the Lua Framework.</Description>
          <MIN>13</MIN>
        </Message>
        <Message>
          <Name>setPassword</Name>
          <Description>Sets a new password value</Description>
          <MIN>14</MIN>
          <Fields>
            <Field xsi:type="StringField">
              <Name>old</Name>
              <Description>The current password value.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>new</Name>
              <Description>The new password value. Empty string to remove the password.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>disableService</Name>
          <Description>Disables, or re-enables, a service.</Description>
          <MIN>15</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>sin</Name>
              <Description>The Service Identification Number for the service to disable/re-enable.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>disable</Name>
              <Description>Disable the service if True, or re-enable it otherwise.</Description>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setEnabledServices</Name>
          <Description>Sets the list of enabled services.</Description>
          <MIN>16</MIN>
          <Fields>
            <Field xsi:type="DataField">
              <Name>sinList</Name>
              <Description>Lists all services that are to be enabled (each data byte represents one SIN).</Description>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setFactoryPassword</Name>
          <Description>Sets a new factory password value.</Description>
          <MIN>247</MIN>
          <Fields>
            <Field xsi:type="StringField">
              <Name>old</Name>
              <Description>The current password value.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>new</Name>
              <Description>The new password value.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
          <Hide>true</Hide>
        </Message>
      </ForwardMessages>
      <ReturnMessages>
        <Message>
          <Name>terminalInfo</Name>
          <Description>Static terminal information.</Description>
          <MIN>1</MIN>
          <Fields>
            <Field xsi:type="EnumField">
              <Name>hardwareVariant</Name>
              <Description>This field indicates the terminal type.</Description>
              <Items>
                <string>Unknown</string>
                <string>IDP-6XX</string>
                <string>IDP-7XX</string>
                <string>IDP-8XX</string>
                <string>IDP-782</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>hardwareRevision</Name>
              <Description>The hardware revision number.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="DataField">
              <Name>hardwareFeatureMask</Name>
              <Description>Bit mask indicating hardware features</Description>
              <Fixed>true</Fixed>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>hardwareManufacturingID</Name>
              <Description>Manufacturing ID of the terminal motherboard.</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>bootLoaderMajor</Name>
              <Description>The major version number of the installed boot loader.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>bootLoaderMinor</Name>
              <Description>The minor version number of the installed boot loader.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>bootLoaderPatch</Name>
              <Description>The patch level number of the installed boot loader.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>bootLoaderDescription</Name>
              <Description>Build description</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwareMajor</Name>
              <Description>The major version number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwareMinor</Name>
              <Description>The minor version number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwarePatch</Name>
              <Description>The patch level number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>firmwareDescription</Name>
              <Description>Build description</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>LSFVersion</Name>
              <Description>Lua services framework version string</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>packageVersion</Name>
              <Description>Version string of the loaded package.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>terminalStatus</Name>
          <Description>Dynamic terminal information.</Description>
          <MIN>2</MIN>
          <Fields>
            <Field xsi:type="SignedIntField">
              <Name>uptime</Name>
              <Description>The number of seconds the terminal has been in continuous operation.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>timeOfDay</Name>
              <Description>The current date &amp; time, expressed as number of seconds since the epoch.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="EnumField">
              <Name>hardwareResetReason</Name>
              <Description>The reason from the hardware for the last terminal reset.</Description>
              <Items>
                <string>Unknown</string>
                <string>PowerOn</string>
                <string>External</string>
                <string>WindowWD</string>
                <string>IndependentWD</string>
                <string>LowPower</string>
                <string>Software</string>
                <string>JTAG</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="EnumField">
              <Name>softwareResetReason</Name>
              <Description>The cause of the software reset.</Description>
              <Items>
                <string>None</string>
                <string>Unknown</string>
                <string>Exception</string>
                <string>CAssert</string>
                <string>CMSISAssert</string>
                <string>AbnormalExit</string>
                <string>KernelError</string>
                <string>Panic</string>
                <string>BootIdle</string>
                <string>BootConsole</string>
                <string>Startup</string>
                <string>Console</string>
                <string>Reserved12</string>
                <string>Reserved13</string>
                <string>Reserved14</string>
                <string>Reserved15</string>
                <string>Reserved16</string>
                <string>Reserved17</string>
                <string>Reserved18</string>
                <string>Reserved19</string>
                <string>Lua</string>
                <string>LuaPanic</string>
                <string>LuaOTA</string>
                <string>LuaShell</string>
                <string>LuaAPI</string>
                <string>LuaSerial</string>
                <string>IDPModem</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>softwareResetDescription</Name>
              <Description>Extra information about the software reset cause.</Description>
              <Size>1000</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>LSFUptime</Name>
              <Description>The number of seconds since the Lua Services Framework started.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>powerSaveTime</Name>
              <Description>The number of seconds the terminal has been in power save mode.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>LSFIdleTime</Name>
              <Description>The number of seconds the Lua Services Framework has been idle waiting for an event since startup.</Description>
              <Size>31</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>serviceList</Name>
          <Description>List of installed terminal services.</Description>
          <MIN>3</MIN>
          <Fields>
            <Field xsi:type="DataField">
              <Name>sinList</Name>
              <Description>Each byte in the data field represents the SIN of an enabled service.</Description>
              <Size>255</Size>
            </Field>
            <Field xsi:type="DataField">
              <Name>disabledList</Name>
              <Description>Each byte in the data field represents the SIN of a disabled service.</Description>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>serviceInfo</Name>
          <Description>Static information for one or more services.</Description>
          <MIN>4</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>sin</Name>
              <Description>The SIN.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>name</Name>
              <Description>The service name, up to 64 characters.</Description>
              <Size>64</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>version</Name>
              <Description>Free-form service version, up to 16 characters.</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>enabled</Name>
              <Description>Whether the service is currently enabled.</Description>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>running</Name>
              <Description>True if the service is running.</Description>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>propertyValues</Name>
          <Description>A list of current property values.</Description>
          <MIN>5</MIN>
          <Fields>
            <Field xsi:type="ArrayField">
              <Name>list</Name>
              <Description>A list of services.</Description>
              <Fields>
                <Field xsi:type="UnsignedIntField">
                  <Name>sin</Name>
                  <Description>The SIN.</Description>
                  <Size>8</Size>
                </Field>
                <Field xsi:type="ArrayField">
                  <Name>propList</Name>
                  <Description>A list of properties for that service.</Description>
                  <Fields>
                    <Field xsi:type="UnsignedIntField">
                      <Name>pin</Name>
                      <Description>The PIN.</Description>
                      <Size>8</Size>
                    </Field>
                    <Field xsi:type="DynamicField">
                      <Name>value</Name>
                      <Description>The property value.</Description>
                    </Field>
                  </Fields>
                  <Size>255</Size>
                </Field>
              </Fields>
              <Size>255</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>termReset</Name>
          <Description>Indicates a terminal reset. This message is only used for logging purposes.</Description>
          <MIN>6</MIN>
        </Message>
        <Message>
          <Name>timeSync</Name>
          <Description>Indicates a time synchronization event. This message is only used for logging purposes.</Description>
          <MIN>7</MIN>
          <Fields>
            <Field xsi:type="SignedIntField">
              <Name>time</Name>
              <Description>The new date &amp; time, expressed as number of seconds since the epoch.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>terminalRegistration</Name>
          <Description>Terminal registration message; automatically sent at system startup.</Description>
          <MIN>8</MIN>
          <Fields>
            <Field xsi:type="EnumField">
              <Name>hardwareVariant</Name>
              <Description>This field indicates the terminal type.</Description>
              <Items>
                <string>Unknown</string>
                <string>IDP-6XX</string>
                <string>IDP-7XX</string>
                <string>IDP-8XX</string>
                <string>IDP-782</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>hardwareRevision</Name>
              <Description>The hardware revision number.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="EnumField">
              <Name>hardwareResetReason</Name>
              <Description>The reason from the hardware for the last terminal reset.</Description>
              <Items>
                <string>Unknown</string>
                <string>PowerOn</string>
                <string>External</string>
                <string>WindowWD</string>
                <string>IndependentWD</string>
                <string>LowPower</string>
                <string>Software</string>
                <string>JTAG</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwareMajor</Name>
              <Description>The major version number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwareMinor</Name>
              <Description>The minor version number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>firmwarePatch</Name>
              <Description>The patch level number of the installed base firmware.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>LSFVersion</Name>
              <Description>Lua services framework version string</Description>
              <Size>16</Size>
            </Field>
            <Field xsi:type="EnumField">
              <Name>softwareResetReason</Name>
              <Description>The cause of the software reset.</Description>
              <Items>
                <string>None</string>
                <string>Unknown</string>
                <string>Exception</string>
                <string>CAssert</string>
                <string>CMSISAssert</string>
                <string>AbnormalExit</string>
                <string>KernelError</string>
                <string>Panic</string>
                <string>BootIdle</string>
                <string>BootConsole</string>
                <string>Startup</string>
                <string>Console</string>
                <string>Reserved12</string>
                <string>Reserved13</string>
                <string>Reserved14</string>
                <string>Reserved15</string>
                <string>Reserved16</string>
                <string>Reserved17</string>
                <string>Reserved18</string>
                <string>Reserved19</string>
                <string>Lua</string>
                <string>LuaPanic</string>
                <string>LuaOTA</string>
                <string>LuaShell</string>
                <string>LuaAPI</string>
                <string>LuaSerial</string>
                <string>IDPModem</string>
              </Items>
              <Size>8</Size>
            </Field>
            <Field xsi:type="DataField">
              <Name>sinList</Name>
              <Description>Each byte in the data field is the SIN of an installed service.</Description>
              <Size>240</Size>
            </Field>
            <Field xsi:type="StringField">
              <Name>packageVersion</Name>
              <Description>Version string of the loaded package.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setPasswordResult</Name>
          <Description>The result from a setPassword message.</Description>
          <MIN>9</MIN>
          <Fields>
            <Field xsi:type="BooleanField">
              <Name>success</Name>
              <Description>Whether the operation succeeded or not.</Description>
            </Field>
            <Field xsi:type="StringField">
              <Name>error</Name>
              <Description>A description of the error, if success is false.</Description>
              <Optional>true</Optional>
              <Size>64</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>disableServiceResult</Name>
          <Description>The result of a disableService request.</Description>
          <MIN>10</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>sin</Name>
              <Description>The Service Identification Number for the service that was disabled/re-enabled.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>disable</Name>
              <Description>Whether an attempt was made to disable or re-enable the service.</Description>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>success</Name>
              <Description>Whether the attempt was successful or not.</Description>
            </Field>
          </Fields>
        </Message>
      </ReturnMessages>
    </Service>
    <Service>
      <Name>idp</Name>
      <SIN>27</SIN>
      <ForwardMessages>
        <Message>
          <Name>getSReg</Name>
          <Description>Retrieves an S-register value.</Description>
          <MIN>1</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>num</Name>
              <Description>The S-register number.</Description>
              <Size>8</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setSreg</Name>
          <Description>Sets an S-register value</Description>
          <MIN>2</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>num</Name>
              <Description>The S-register number.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>val</Name>
              <Description>The desired S-register value.</Description>
              <Size>32</Size>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>getMetrics</Name>
          <Description>Retrieves a metrics block from the modem based on the class and subclass.</Description>
          <MIN>3</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>class</Name>
              <Description>The class of the desired metrics block.</Description>
              <Size>3</Size>
            </Field>
            <Field xsi:type="UnsignedIntField">
              <Name>subclass</Name>
              <Description>The subclass of the desired metrics block.</Description>
              <Size>5</Size>
            </Field>
          </Fields>
        </Message>
      </ForwardMessages>
      <ReturnMessages>
        <Message>
          <Name>getSRegResult</Name>
          <Description>The result of a getSReg operation.</Description>
          <MIN>1</MIN>
          <Fields>
            <Field xsi:type="UnsignedIntField">
              <Name>num</Name>
              <Description>The S-register number.</Description>
              <Size>8</Size>
            </Field>
            <Field xsi:type="SignedIntField">
              <Name>val</Name>
              <Description>The S-register value.</Description>
              <Size>32</Size>
            </Field>
            <Field xsi:type="BooleanField">
              <Name>success</Name>
              <Description>Indicates success of the get operation.</Description>
            </Field>
          </Fields>
        </Message>
        <Message>
          <Name>setSRegResult</Name>
          <Description>The result of a setSReg operation.</Description>
